<Window x:Class="StockTickerExtension2019.FuzzySearchDialog"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Search" MaxHeight="350" MinHeight="150" Width="150"
        WindowStyle="None" AllowsTransparency="True"
        ShowInTaskbar="False" Topmost="True"
        WindowStartupLocation="Manual" Height="185" >
    <Border BorderThickness="0" CornerRadius="3">
        <ListBox x:Name="ResultList" DisplayMemberPath="DisplayText" MouseDoubleClick="ListBox_MouseDoubleClickEvent" Margin="0,0,0,0"/>
    </Border>

</Window>
